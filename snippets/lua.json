{
	"require(...)": {
		"prefix": "require",
		"body": "require('$1').setup($0)",
		"desc": "Lua require statement."
	},
	"neovim set keymap": {
		"prefix": "setkeymap",
		"body": "vim.api.nvim_set_keymap('n', '$1', { noremap = true, silent = true})",
		"desc": "Neovim set keymap statement via vim api."
	}
}
